/*
 * Propose de calculer la centralité en se basant sur les chemins faisant partie d'une solution du calcul de flot max.
 * Peut être intéressant dans certain cas, mais pas pour notre problème.
 * Effectivement, supposons qu'on atteigne le flot max sur un arc,  on va donc choisir tout les prochains chemins de façon à 
 * contourner cet arc. Or si dans la pratique cet arc n'est jamais encombrer, mais qu'il est la source d'un nombre très important 
 * de chemin très interessant vers une destination (ou plusieurs) alors le noeud à l'extrémité de cet arc aura une centralité 
 * potentiellement plus faible alors que ca ne devrait pas.
 * On propose donc ici une solution à un problème qui ne nous interesse pas.
 */
@article{FreemanBorgattiWhite1991Centrality,
	title={Centrality in valued graphs: A measure of betweenness based on network flow},
	volume={13},
	number={2},
	journal={Social Networks},
	publisher={Elsevier},
	author={Freeman, L. C. and Borgatti, S. P. and White, D. R.},
	year={1991},
	pages={141--154}
}

/*
 * L'auteur fait un tour d'horizon des mesures de centralité dans des réseaux modèlisant des flots.
 * Il montre que certains réseaux ont des propriété très différentes, et par conséquent les mesures de centralité ne vont pas avoir 
 * la même efficatité. Il montre d'ailleurs que la centralité d'intermédiarité n'est pas forcément appropriée lorsqu'on cherche la 
 * centralité d'un noeud dans un réseaux où les "chose" qui s'y déplacent n'emprunte pas le plus court chemin, comme c'est le cas 
 * pour un réseaux routier.
 *
 * De plus, la mesure de Freeman adapter pour les flots n'est pas non plus toujours approprié car selon la sémantique du graphe, le 
 * processus de calcul de flot n'est pas toujours pareil et il faut donc adapter le calcul de la centralité à ces cas particulier.
 * Borgatti prend ainsi des exemples montrant des caractéristiques générales.
 * 
 * Il compare des méthodes ressemblant à des marche aléatoire avec les calcul exacte. Il montre que les résultats sont similaires.
 * => a vérifier (voir la partie sur la betweenness et la méthode de "package")
 */
@article{Borgatti2005Centrality,
	author = {Borgatti, S. P.},
	journal = {Social Networks},
	month = jan,
	number = {1},
	pages = {55--71},
	publisher = {Elsevier},
	title = {{Centrality and network flow}},
	volume = {27},
