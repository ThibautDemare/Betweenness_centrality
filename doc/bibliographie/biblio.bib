}

/*
 * Résume certaines idées directrices sur la centralité dans les graphes et rappel l'existence de technique plus exotique. Notamment :
 * - k-path centrality : compte tout les chemin de taille k ou moins partant d'un noeud donné. Si k=1, equivalent au degré, et quand 
 *   k=n-1 on obtient tout les chemins sans exeption.
 * - edge-disjoint k-path-centrality : identique au k-path à la difference que les chemins sont disjoint entre eux (ne partage pas 
 *   la même arrete).
 * - vertex-disjoint k-path centrality : idem que precedemment mais cette fois les chemins ne partagent pas les noeud (sauf les 
 *   extremités).
 * - Katz's measure : on compte tout les chemins possibles et on les pondère : plus ils sont long et indirecte, plus ils sont peu 
 *   pondérer, et à l'inverse, plus il sont court et directe, plus ils sont fortement pondéré.
 *   Il existe des variantes :
 *     -> Hubbell (1965 et repris semble-t-il par Hoed 1978)
 *     -> Friedkin (1991)
 *     -> Bonacich (1987)
 */
@article{Borgatti2006GraphTheoretic,
    author = {Borgatti, S. P. and Everett, R.},
    journal = {Social Networks},
    title = {A graph-theoretic perspective on centrality},
    year = {2006}
}

